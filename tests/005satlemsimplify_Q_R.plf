(check 
  (% v1 var
  (% v2 var
    (% p1 (holds (clc (pos v1) (clc (pos v2) cln)))
    (% p2 (holds (clc (neg v1) (clc (pos v2) cln)))
    (% p3 (holds (clc (neg v2) cln))
      (: (holds cln)
            (satlem_simplify _ _ _ (R _ _ p1 p2 v1)
              (\ p4
                (satlem_simplify _ _ _ (Q _ _ p3 p4 v2)
                  (\ p5 p5)))))))))))
